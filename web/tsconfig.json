/* Visit https://aka.ms/tsconfig to read more about this file */
{
    "compilerOptions": {
        "target": "ES2020",
        "jsx": "react",
        "module": "commonjs",
        "rootDir": "src",
        "esModuleInterop": true,
        "forceConsistentCasingInFileNames": true,
        "strict": true,
        "skipLibCheck": true,
        "noImplicitAny": true,
        "noImplicitThis": true
    },
    "exclude": ["scripts", "dist"]
}
