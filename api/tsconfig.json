/* Visit https://aka.ms/tsconfig to read more about this file */
{
    "compilerOptions": {
        "target": "ESNext",
        "lib": ["ESNext"],
        "module": "ESNext",
        "moduleResolution": "bundler",
        "outDir": "dist",
        "esModuleInterop": true,
        "forceConsistentCasingInFileNames": true,
        "strict": true,
        "skipLibCheck": true,
        "importHelpers": true,
        "types": ["node"]
    },
    "exclude": ["dist"]
}
